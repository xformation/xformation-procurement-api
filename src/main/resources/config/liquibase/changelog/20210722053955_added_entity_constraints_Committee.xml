<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Committee.
    -->
    <changeSet id="20210722053955-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="committee_id"
                                 baseTableName="committee_contact"
                                 constraintName="fk_committee_contact_committee_id"
                                 referencedColumnNames="id"
                                 referencedTableName="committee"/>

        <addForeignKeyConstraint baseColumnNames="contact_id"
                                 baseTableName="committee_contact"
                                 constraintName="fk_committee_contact_contact_id"
                                 referencedColumnNames="id"
                                 referencedTableName="contact"/>
        
    </changeSet>
</databaseChangeLog>
